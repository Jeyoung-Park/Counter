//타입스크립트 설정
{
    "compilerOptions":{
        "allowJs":true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react",
        "lib": ["es6"],
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "target":"ESNext",
        "baseUrl": "./src",
        "paths": {
            "-/*":["*"]
        }
    },
    "exclude":[
        "node_module",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js"
    ]
}